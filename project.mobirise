          "_name": "carousel1",
          "_sourceTheme": "gadgetamp",
          "_customHTML": "<section class=\"carousel1 slider testimonials-carousel\" group=\"Carousels &amp; Sliders\" plugins=\"amp-main, amp-carousel\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n        <header>Cards</header>\n        <input type=\"color\" title=\"Cards Bg\" name=\"cardsBg\" value=\"#f4f4f4\">    \n        <input type=\"checkbox\" title=\"ShowCardsSignature\" name=\"ShowCardsSignature\">\n        <input type=\"checkbox\" title=\"ShowCardImg\" name=\"ShowCardImg\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Cards Height\" name=\"carouselHeight\" min=\"200\" max=\"1000\" step=\"10\" value=\"460\">\n        <input type=\"range\" inline=\"\" title=\"Cards\" name=\"imagesAmount\" min=\"2\" max=\"7\" step=\"1\" value=\"2\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/gadget-3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f6efe9\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    \n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\"></div>\n    <!-- <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div> -->\n    <div class=\"\" mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"text__block mbr-col-sm-12 mbr-col-md-8 mbr-m-auto\" mbr-if=\"showTitle || showSubtitle\">\n            \n            <h2 class=\"mbr-fonts-style mbr-section-title align-center mbr-pb-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n              Innovative Smarter Brighter</h2>\n        </div>\n        <div class=\"slider-box\"><amp-carousel class=\"carousel\" height=\"{{carouselHeight}}\" layout=\"fixed-height\" type=\"carousel\"><div class=\"text-wrap\">\n                    <div class=\"text-content shadow\">\n                        <div class=\"card-img\" mbr-if=\"ShowCardImg\">\n                          <amp-img src=\"@PROJECT_PATH@/assets/images/mbr-468x468.png\" alt=\"image\" height=\"460\" width=\"460\" layout=\"responsive\" class=\"mobirise-loader\">\n                          <div placeholder=\"\" class=\"placeholder\"></div>\n                          <img src=\"@PROJECT_PATH@/assets/images/mbr-468x468.png\" class=\"mbr-temp\" alt=\"\" title=\"\">\n                          </amp-img>\n                        </div>\n                        <p class=\"mbr-fonts-style mbr-text card-text align-center\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text, .card-signature\">\n                            Numéro 1<br>Octobre 2020</p>\n                        <div class=\"testimonial-info mbr-flex mbr-jc-sb mbr-pb-4\" mbr-if=\"ShowCardTitle || ShowCardsSignature || ShowCardImg\">\n                        <p class=\"mbr-fonts-style mbr-text align-center ca