        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "amp-img,img": {
              "height": "auto",
              "width": "100%"
            },
            ".gallery-row": {
              "& when not (@flexDirection)": {
                "padding-left": "0",
                "padding-right": "0"
              },
              "@media (max-width: 992px)": {
                "justify-content": "center"
              }
            },
            ".gallery-item": {
              "& when (@flexDirection)": {
                "padding-left": "0",
                "padding-right": "0"
              }
            },
            ".gallery-row, .gallery-item": {
              "padding-left": "0.5rem",
              "padding-right": "0.5rem",
              "@media (max-width: 768px)": {
                "padding-left": "0rem",
                "padding-right": "0rem"
              }
            },
            ".gallery-item, .gallery-row": {
              "&:nth-last-child(1)": {
                "padding-bottom": "0"
              }
            },
            "& when not (@paddingCard)": {
              ".gallery-item, .gallery-row": {
                "padding": "0!important"
              }
            },
            ".gallery-img-wrap": {
              "position": "relative",
              "& when (@captionHover)": {
                "&:hover": {
                  ".overlay-caption": {
                    "right": "0",
                    "opacity": "@imgOverlayOpacity"
                  }
                }
              },
              "& when not (@captionHover)": {
                "&:hover": {
                  ".overlay-caption": {
                    "right": "-100%",
                    "opacity": "0"
                  }
                }
              },
              "&::after": {
                "background": "none"
              }
            },
            ".overlay-caption": {
              "align-items": "center",
              "flex-direction": "column",
              "padding": "20px 10px 30px 30px",
              "opacity": "0",
              "position": "absolute",
              "bottom": "0",
              "right": "-100%",
              "left": "auto",
              "max-height": "100%",
              "width": "80%",
              "background-color": "@imgOverlayColor",
              "border-left": "2px solid @borderColor",
              "transition": "1s",
              "& when not (@captionHover)": {
                "right": "0",
                "opacity": "1"
              }
            },
            "& when not (@fullWidth)": {
              ".overlay-caption": {
                "padding": "10px"
              }
            },
            ".mbr-row": {
              "padding-left": "0rem",
              "padding-right": "0rem",
              "@media (max-width: 767px)": {
                "padding-left": "1rem",
                "padding-right": "1rem"
              }
            },
            "h4": {
              "width": "100%",
              "padding-right": "8px",
              "padding-left": "8px"
            },
            "@media (max-width: 991px)": {
              ".overlay-caption": {
                "opacity": "1",
                "right": "0"
              },
              ".gallery-img-wrap": {
                "& when (@captionHover)": {
                  "&:hover": {
                    ".overlay-caption": {
                      "right": "-100%",
                      "opacity": "0"
                    }
                  }
                }
              }
            }
          },
